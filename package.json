{
  "name": "node-koa-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "pm2": "pm2",
    "knex": "knex --knexfile ./knex/knexfile.migrate.ts",
    "migrate": "yarn knex migrate:latest",
    "migration:make": "knex migrate:make",
    "migration:rollback": "yarn knex migrate:rollback",
    "start:dev": "yarn pm2 startOrRestart pm2/development.json",
    "start:debug": "yarn pm2 startOrRestart pm2/debug.json",
    "start:prod": "yarn pm2 startOrRestart pm2/production.json",
    "build": "rm -rf build/ & tsc & cp .env build/.env",
    "lint": "eslint app --ext .ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/joi": "^14.3.3",
    "@types/koa": "^2.0.51",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-passport": "^4.0.2",
    "@types/koa-router": "^7.0.42",
    "@types/node": "^12.11.2",
    "@types/passport-local": "^1.0.33",
    "@typescript-eslint/parser": "^2.5.0",
    "chai": "4.1.1",
    "chai-http": "^3.0.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "koa-bodyparser": "^4.3.0",
    "koa-compose": "^4.1.0",
    "lint-staged": "^9.4.2",
    "mocha": "3.5.0",
    "pm2": "^4.1.2",
    "prettier": "^1.18.2",
    "sinon": "^4.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "google-protobuf": "^3.12.2",
    "grpc": "^1.24.3",
    "grpc-tools": "^1.9.0",
    "grpc-web": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-passport": "^4.0.1",
    "koa-redis": "^3.1.1",
    "koa-router": "^7.2.1",
    "koa-session": "^5.5.1",
    "mongodb": "^3.5.9",
    "mongoose": "^5.9.22",
    "objection": "^2.1.3",
    "objection-timestamps": "^4.0.0",
    "objection-visibility": "^1.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.0.3",
    "ts-protoc-gen": "^0.12.0",
    "winston": "^3.3.3",
    "@koa/cors": "^3.0.0",
    "@sentry/node": "^5.7.1",
    "joi": "^14.3.1",
    "koa-compose": "^4.1.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "mysql": "^2.17.1",
    "source-map-support": "^0.5.13"
  },
  "lint-staged": {
    "app/**/*.ts": [
      "prettier --write",
      "git add",
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
